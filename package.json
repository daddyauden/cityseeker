  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/runtime": "^7.5.0",
    "@storybook/addon-actions": "5.1.10",
