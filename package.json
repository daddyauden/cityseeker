    "lint": "eslint .",
    "ci": "npm run precommit && codecov",
    "ios": "react-native run-ios",
    "log-android": "react-native log-android",
    "android": "react-native run-android",
    "snyk-protect": "snyk protect",
    "format": "prettier --tab-width 4 --write 'src/**/*.js'",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --reset-cache --bundle-output './ios/cityseeker/main.jsbundle' --assets-dest='ios/'",
    "bundle:android": "react-native bundle --entry-file index.js --platform android --dev false --reset-cache --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "postinstall": "patch-package && jetify"
  },
